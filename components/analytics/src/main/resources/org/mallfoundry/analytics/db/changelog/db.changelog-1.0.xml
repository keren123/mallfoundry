<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="1.0.0" author="mallfoundry">
        <createTable tableName="mf_dw_date_dimension">
            <column name="id_" type="varchar(255)"/>
            <column name="year_" type="integer"/>
            <column name="month_" type="integer"/>
            <column name="day_of_month_" type="integer"/>
        </createTable>
        <createTable tableName="mf_dw_product_fact">
            <column name="store_id_" type="varchar(255)"/>
        </createTable>
        <createTable tableName="mf_dw_order_quantity_fact">
            <column name="store_id_" type="varchar(255)"/>
            <column name="date_id_" type="varchar(255)"/>
            <column name="product_id_" type="varchar(255)"/>
            <column name="customer_id_" type="varchar(255)"/>
            <column name="awaiting_payment_quantity_" type="integer"/>
            <column name="awaiting_shipment_quantity_" type="integer"/>
            <column name="disputed_quantity_" type="integer"/>
        </createTable>
    </changeSet>
</databaseChangeLog>