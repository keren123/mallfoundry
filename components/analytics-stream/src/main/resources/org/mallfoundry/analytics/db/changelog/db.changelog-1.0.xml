        <createTable tableName="mf_dw_product_fact">
            <column name="id_" type="varchar(255)"/>
            <column name="store_id_" type="varchar(255)"/>
            <column name="brand_id_" type="varchar(255)"/>
            <column name="category_id_" type="varchar(255)"/>
            <column name="status_id_" type="varchar(25)"/>
