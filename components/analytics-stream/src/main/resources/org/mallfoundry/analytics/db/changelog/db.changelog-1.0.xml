<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="1.0.0" author="mallfoundry">
        <createTable tableName="mf_dw_date_dimension">
            <column name="id_" type="varchar(255)"/>
            <column name="year_" type="integer"/>
            <column name="month_" type="integer"/>
            <column name="day_of_month_" type="integer"/>
            <column name="quarter_" type="integer"/>
        </createTable>
        <createTable tableName="mf_dw_order_item_fact">
            <column name="id_" type="varchar(255)"/>
            <column name="order_id_" type="varchar(255)"/>
            <column name="store_id_" type="varchar(255)"/>
            <column name="customer_id_" type="varchar(255)"/>
            <column name="product_id_" type="varchar(255)"/>
            <column name="variant_id_" type="varchar(255)"/>
            <column name="status_id_" type="varchar(255)"/>
            <column name="placed_date_id_" type="varchar(255)"/>
            <column name="paid_date_id_" type="varchar(255)"/>
            <column name="shipped_date_id_" type="varchar(255)"/>
            <column name="cancelled_date_id_" type="varchar(255)"/>
            <column name="quantity_" type="varchar(255)"/>
            <column name="price_" type="varchar(255)"/>
            <column name="shipping_cost_" type="varchar(255)"/>
            <column name="subtotal_amount_" type="varchar(255)"/>
            <column name="total_amount_" type="varchar(255)"/>
        </createTable>
        <createTable tableName="mf_dw_order_quantity_fact">
            <column name="store_id_" type="varchar(255)"/>
            <column name="customer_id_" type="varchar(255)"/>
            <column name="product_id_" type="varchar(255)"/>
            <column name="variant_id_" type="varchar(255)"/>
            <column name="status_id_" type="varchar(25)"/>
            <column name="quantity_" type="integer" defaultValue="0"/>
        </createTable>
